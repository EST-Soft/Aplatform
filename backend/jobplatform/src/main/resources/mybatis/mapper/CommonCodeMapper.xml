<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.community.enter.domain.mapper.CommonCodeMapper">
	
	<!-- 지원자 상태 얻기 -->
	<select id="selectCommonCodeApplyCategoryData" resultType="com.community.enter.domain.vo.CommonCodeVO">
		select code_id, code_name
		from P2_TBL_COMMON_CODE_DTL_S 
		where code_ctgry_id = 400 
			and code_id != 403;
	</select>
	
	

</mapper>